      },
      "Retry": [
        {
          "ErrorEquals": [
            "Lambda.ServiceException",
            "Lambda.AWSLambdaException",
            "Lambda.SdkClientException"
          ],
          "IntervalSeconds": 2,
          "MaxAttempts": 6,
          "BackoffRate": 2
        }
      ],
      "Next": "getting confidence inference"
    },
    "getting confidence inference": {
      "Type": "Task",
      "Resource": "arn:aws:states:::lambda:invoke",
