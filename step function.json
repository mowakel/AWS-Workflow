{
  "Comment": "A description of my state machine",
  "StartAt": "Serializing Image Data",
  "States": {
    "Serializing Image Data": {
      "Type": "Task",
      "Resource": "arn:aws:states:::lambda:invoke",
      "OutputPath": "$.Payload",
      "Parameters": {
        "Payload.$": "$",
        "FunctionName": "arn:aws:lambda:us-east-1:774432409850:function:mylambda:$LATEST"
      },
      "Retry": [
        {
          "ErrorEquals": [
            "Lambda.ServiceException",
            "Lambda.AWSLambdaException",
            "Lambda.SdkClientException"
          ],
          "IntervalSeconds": 2,
          "MaxAttempts": 6,
          "BackoffRate": 2
        }
      ],
      "Next": "classifing them"
    },
    "classifing them": {
      "Type": "Task",
      "Resource": "arn:aws:states:::lambda:invoke",
